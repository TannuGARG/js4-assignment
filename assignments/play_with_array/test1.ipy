def geteven(arr):
    i=0
    arr1=[]
    while i<len(arr):
        if arr[i]%2==0:
            arr1.apend(arr[i])
        i+=1
    return arr1

def multiplybyn(arr,n):
    for i in range(len(arr)):
        arr[i]=arr[i]*n
    return arr

def removenthelement(arr,n):
    arr1=[]
    for i in range(len(arr)):
        if i!=n:
            arr1.append(arr[i])
    return arr1



